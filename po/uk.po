#: src/mpop.c:90
#, c-format
msgid "%s: FATAL: %s\n"
msgstr "%s: КРИТИЧНА ПОМИЛКА: %s\n"

#: src/mpop.c:231 src/mpop.c:748
#, c-format
msgid "the POP3 server does not support TLS via the STLS command"
msgstr "на сервері POP3 не передбачено підтримки TLS на основі команди STLS"

#: src/mpop.c:300
#, c-format
msgid "POP3 server at %s (%s [%s]), port %d:\n"
msgstr "Сервер POP3 на %s (%s [%s]), порт %d:\n"

#: src/mpop.c:305
#, c-format
msgid "POP3 server at %s (%s), port %d:\n"
msgstr "Сервер POP3 на %s (%s), порт %d:\n"

#: src/mpop.c:310
#, c-format
msgid "POP3 server at %s ([%s]), port %d:\n"
msgstr "Сервер POP3 на %s ([%s]), порт %d:\n"

#: src/mpop.c:315
#, c-format
msgid "POP3 server at %s, port %d:\n"
msgstr "Сервер POP3 на %s, порт %d:\n"

#: src/mpop.c:327
#, c-format
msgid "POP3 capabilities:\n"
msgstr "Можливості POP3:\n"

#: src/mpop.c:331
msgid "Support for the CAPA command (get list of capabilities)"
msgstr "Підтримка команди CAPA (отримання списку можливостей)"

#: src/mpop.c:341
msgid "Support for command grouping for faster transmission"
msgstr "Підтримка групування команд для швидшого передавання даних"

#: src/mpop.c:346
msgid "Support for the TOP command (get mail headers)"
msgstr "Підтримка команди TOP (отримання заголовків пошти)"

#: src/mpop.c:351
msgid "Support for the UIDL command (get unique mail identifiers)"
msgstr "Підтримка команди UIDL (отримання унікальних ідентифікаторів пошти)"

#: src/mpop.c:360
#, c-format
msgid "minimum time between logins is %ld seconds = %.2f hours"
msgstr "мінімальний час між спробами входу дорівнює %ld секунд = %.2f годин"

#: src/mpop.c:365
#, c-format
msgid "minimum time between logins is %ld seconds = %.2f minutes"
msgstr "мінімальний час між спробами входу дорівнює %ld секунд = %.2f хвилин"

#: src/mpop.c:370
#, c-format
msgid "minimum time between logins is %ld seconds"
msgstr "мінімальний час між спробами входу дорівнює %ld секунд"

#: src/mpop.c:381
msgid "this POP3 server will never delete mails"
msgstr "цей сервер POP3 ніколи не вилучатиме повідомлення"

#: src/mpop.c:386
msgid "this POP3 server will not keep mails"
msgstr "цей сервер POP3 не зберігатиме повідомлення"

#: src/mpop.c:391
#, c-format
msgid "this POP3 server will keep mails for %ld days"
msgstr "цей сервер POP3 зберігатиме повідомлення протягом %ld днів"

#: src/mpop.c:404
msgid "Support for TLS encryption via the STLS command"
msgstr "Підтримка шифрування TLS на основі команди STLS"

#: src/mpop.c:407
msgid "Supported authentication methods:"
msgstr "Підтримувані способи розпізнавання:"

#: src/mpop.c:464 src/mpop.c:470
msgid "Server error messages in square brackets have a special meaning"
msgstr ""
"Повідомлення про помилки сервера у квадратних дужках мають особливе значення"

#: src/mpop.c:479
#, c-format
msgid ""
"This server might advertise more or other capabilities\n"
"    when TLS is active.\n"
msgstr ""
"Можливості цього сервера можуть бути ширшими, якщо\n"
"    увімкнено TLS.\n"

#: src/mpop.c:484
#, c-format
msgid ""
"This server might advertise more or other capabilities\n"
"    after successful authentication.\n"
msgstr ""
"Можливості цього сервера можуть бути ширшими, якщо\n"
"    вдасться успішно пройти розпізнавання.\n"

#: src/mpop.c:489
#, c-format
msgid ""
"This server does not support the CAPA command, so this\n"
"    list is probably not complete.\n"
msgstr ""
"На цьому сервері не передбачено підтримки команди CAPA, тому\n"
"    цей список, ймовірно, є неповним.\n"

