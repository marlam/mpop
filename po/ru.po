msgstr ""
"  --tls-certcheck[=(on|off)]   включить/отключить проверку сертификатов для "
"TLS\n"

#: src/mpop.c:2294
#, c-format
msgid "  --tls-key-file=[file]        set/unset private key file for TLS\n"
msgstr ""
"  --tls-key-file=[файл]        указать/сбросить файл закрытого ключа для "
"TLS\n"

#: src/mpop.c:2295
#, c-format
msgid "  --tls-cert-file=[file]       set/unset private cert file for TLS\n"
msgstr ""
"  --tls-cert-file=[файл]       указать/сбросить файл закрытого сертификата "
"для TLS\n"

#: src/mpop.c:2296
#, c-format
msgid "  --tls-priorities=[prios]     set/unset TLS priorities.\n"
msgstr ""
"  --tls-priorities=[prios]     указать/сбросить значения приоритетов TLS.\n"

#: src/mpop.c:2297
#, c-format
msgid ""
"  --tls-host-override=[host]   set/unset override for TLS host "
